<head>
<meta charset="UTF-8">
<title>Insert title here</title>
</head>
<body>
	<% int col = Integer.parseInt(request.getParameter("col")); %>
	<% 
	for(int i=0;i<9/col;i++){
		for(int j=1;j<=9;j++){
			for(int k=i*col;k<k+col;k++){
				out.print(String.format("%2d * %2d = %2d",j,i,j*i));
			}
			out.print("<br/>");
		}
		out.print("<br/>");
	}
	%>
</body>
</html>